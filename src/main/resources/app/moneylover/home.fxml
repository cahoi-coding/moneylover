<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="450.0" prefWidth="650.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.moneylover.controllers.HomeController">
   <children>
      <Button fx:id="homeBtn" layoutX="-2.0" layoutY="-1.0" mnemonicParsing="false" onAction="#switchToSceneHome" prefHeight="25.0" prefWidth="164.0" text="HOME" />
      <Button fx:id="transactionsBtn" layoutX="325.0" layoutY="-1.0" mnemonicParsing="false" onAction="#switchToSceneTransactions" prefHeight="25.0" prefWidth="164.0" text="TRANSACTIONS" />
      <Button fx:id="myWalletsBtn" layoutX="161.0" layoutY="-1.0" mnemonicParsing="false" onAction="#switchToSceneMyWallets" prefHeight="25.0" prefWidth="164.0" text="MY WALLETS" />
      <Button fx:id="statisticalBtn" layoutX="486.0" layoutY="-1.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="164.0" text="STATISTICAL" />
      <ImageView fitHeight="104.0" fitWidth="107.0" layoutX="18.0" layoutY="42.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="http://product.hstatic.net/1000304920/product/joji-nectar-dia-cd_c21dc861348c484db90e91e0c48761c4_grande.jpg" />
         </image>
      </ImageView>
      <Label fx:id="fullNameLabel" layoutX="135.0" layoutY="50.0">
         <font>
            <Font name="System Bold" size="24.0" />
         </font>
      </Label>
      <Button layoutX="135.0" layoutY="108.0" mnemonicParsing="false" text="edit info" />
      <Label layoutX="18.0" layoutY="167.0" text="Recent Transaction" textFill="#221010c7">
         <font>
            <Font name="System Bold" size="13.0" />
         </font>
      </Label>
      <TableView layoutX="18.0" layoutY="194.0" prefHeight="238.0" prefWidth="614.0">
        <columns>
          <TableColumn prefWidth="108.0" text="time" />
          <TableColumn prefWidth="197.0" text="purpose" />
            <TableColumn prefWidth="152.0" text="money" />
            <TableColumn prefWidth="156.0" text="wallet" />
        </columns>
      </TableView>
      <Label layoutX="136.0" layoutY="85.0" text="Total money:">
         <font>
            <Font size="11.0" />
         </font>
      </Label>
      <Label layoutX="203.0" layoutY="83.0" text="200000" textFill="#00ff26">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
